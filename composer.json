    "后盾人",
    "向军老师",
    "邮件处理"
  ],
  "homepage": "http://www.houdunwang.com/",
  "license": "MIT",
  "authors": [
    {
      "name": "后盾网向军",
      "email": "2300071698@qq.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=5.4.0",
    "houdunwang/config": "~1.0",
    "houdunwang/db": "~1.0",
    "houdunwang/arr": "~1.0",
    "houdunwang/dir": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.1"
  },
  "autoload": {
    "files": [
      "src/helper.php"
    ],
    "psr-4": {
      "houdunwang\\cache\\": "src"
    }
